{
  "name": "vue3-laravel-custom-pagination",
  "version": "2.0.0",
  "description": "A Laravel Paginator for vue3 components",
  "author": "Bruno Gritti <glbrunogl@gmail.com>",
  "main": "dist/vue-laravel-pagination.ssr.js",
  "browser": "dist/vue-laravel-pagination.esm.js",
  "module": "dist/vue-laravel-pagination.esm.js",
  "unpkg": "dist/vue-laravel-pagination.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js"
  },
  "keywords": [
    "vue",
    "pagination",
    "laravel"
  ],
  "dependencies": {
    "vue": "^3.4.23"
  },
  "engines": {
    "node": ">=12"
  }
}
